{
  "name": "LMFDB",
  "build": {
      "dockerfile": "Dockerfile"
  },
  "hostRequirements": {
    "cpus": 4
  },
  "postCreateCommand": "/bin/bash .devcontainer/setup.sh",
  "postAttachCommand": "sage -python start-lmfdb.py --debug",
  "customizations": {
    "codespaces": {
      "openFiles": ["GettingStarted.md"]
    },
    "vscode": {
      "extensions": [
        "Lov3.sagemath-enhanced"
      ]
    }
  }
}